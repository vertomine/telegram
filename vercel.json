{
  "version": 2,
  "functions": {
    "api/auth.js": {
      "maxDuration": 30
    },
    "api/health.js": {
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/api/health",
      "dest": "/api/health.js"
    },
    {
      "src": "/api/auth",
      "dest": "/api/auth.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/auth.js"
    }
  ]
}
